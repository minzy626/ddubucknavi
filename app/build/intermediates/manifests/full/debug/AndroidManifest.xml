<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hurryup.traffic.junga.hahaha"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flaticon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.hurryup.traffic.junga.hahaha.splash.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hurryup.traffic.junga.hahaha.main.MainActivity" >
        </activity>
        <activity android:name="com.hurryup.traffic.junga.hahaha.main.EndSearchActivity" />
        <activity
            android:name="com.hurryup.traffic.junga.hahaha.main.ListSearchActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.hurryup.traffic.junga.hahaha.main.ListSearchActivity_end"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.hurryup.traffic.junga.hahaha.main.StartSearchActivity"
            android:launchMode="standard" />
        <activity android:name="com.hurryup.traffic.junga.hahaha.route.RouteActivity" >
        </activity>
        <activity android:name="com.hurryup.traffic.junga.hahaha.result.ResultActivity" />
        <activity android:name="com.hurryup.traffic.junga.hahaha.map.MapActivity" />
    </application>

</manifest>